<resources>

    <!-- Base application theme. -->
    <style name="AppThemeLightBasic0" parent="Theme.AppCompat.NoActionBar">
        <!--<item name="android:statusBarColor">@android:color/transparent</item>-->
        <!--<item name="android:navigationBarColor">@android:color/transparent</item>-->
        <!--<item name="android:windowTranslucentStatus">true</item>-->
        <!--<item name="android:windowTranslucentNavigation">true</item>-->

        <item name="colorSwitchThumbNormal">@color/dark</item>
        <!-- Inactive track color(30% transparency) -->
        <item name="android:colorForeground">@color/dark</item>

        <!--override text color here-->
        <item name="android:textColorPrimary">@color/black1</item>

        <!-- Customize your theme here. -->
        <!-- colorPrimary is used for the default action bar background -->
        <item name="colorPrimary">@color/nice_dark</item>
        <!-- colorPrimaryDark is used for the status bar -->
        <item name="colorPrimaryDark">@color/nice_dark</item>
        <!-- colorAccent is used as the default value for colorControlActivated,
         which is used to tint widgets -->
        <item name="colorAccent">@color/nice_dark</item>

        <!--the color of app bar, button,...-->
        <item name="accent_color">@color/nice_dark</item>
        <!--the color of text view, text box-->
        <item name="text_color">@color/colorPrimaryDark</item>
        <!--the opposite color if text-->
        <item name="them_color_opp">@color/white</item>
        <!--button customize color if possible-->
        <item name="background_customize">@color/bg_customize</item>
        <!--override default background color of theme-->
        <item name="background_color">@color/white</item>


    </style>
